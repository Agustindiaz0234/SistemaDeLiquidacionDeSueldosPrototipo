<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.EmpresaListController">
   <children>
      <AnchorPane prefHeight="404.0" prefWidth="600.0">
         <children>
            <TableView fx:id="tblEmpresas" prefHeight="347.0" prefWidth="603.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <columns>
                <TableColumn fx:id="columNombre" prefWidth="143.0" text="Nombre" />
                <TableColumn fx:id="columDireccion" prefWidth="152.0" text="Direccion" />
                  <TableColumn fx:id="columnTelefono" prefWidth="170.0" text="Telefono" />
                  <TableColumn fx:id="columBoton" prefWidth="134.0" text="Seleccionar" />
              </columns>
            </TableView>
            <Button fx:id="btnEliminar" mnemonicParsing="false" onAction="#navegarEliminarEmpresa" prefHeight="40.0" prefWidth="94.0" text="Eliminar" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="225.0" />
            <Button fx:id="btnEditar" mnemonicParsing="false" onAction="#navegarEditarEmpresa" prefHeight="40.0" prefWidth="94.0" text="Editar" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="115.0" />
            <Button fx:id="btnAgregar" mnemonicParsing="false" onAction="#navegarAgregarEmpresa" prefHeight="40.0" prefWidth="94.0" text="Agregar" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
