<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="400.0" prefWidth="486.0" spacing="10.0" alignment="CENTER" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.EmpresaDetailController">
    <children>
         <Label text="Nombre de la empresa" />
        <HBox spacing="10.0" alignment="CENTER">
           
            <TextField fx:id="txtNombre" prefWidth="200.0" />
        </HBox>
                <Label text="Dirección" />
        <HBox spacing="10.0" alignment="CENTER">
    
            <TextField fx:id="txtDireccion" prefWidth="200.0" />
        </HBox>
          <Label text="Número telefónico" />
        <HBox spacing="10.0" alignment="CENTER">
          
            <TextField fx:id="txtTelefono" prefWidth="200.0" />
        </HBox>
         <Label text="ID" />
        <HBox spacing="10.0" alignment="CENTER">
           
            <TextField fx:id="txtId" prefWidth="200.0" />
        </HBox>
        
        <Button fx:id="btnGuardar" mnemonicParsing="false" onAction="#handleGuardar" text="Guardar" />
    </children>
</VBox>
